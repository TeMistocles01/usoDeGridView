<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.antonio.gesapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">



        <activity android:name=".MainActivity"
            android:configChanges="orientation|keyboard"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_main"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


        <activity
            android:name=".LoginDeConductorActivity"
            android:label="@string/title_activity_login_de_conductor"
            android:screenOrientation="portrait"
            />

        <activity
            android:name=".ListaDeVehiculosRegistradosActivity"
            android:label="@string/title_activity_lista_de_vehiculos_registrados"
            android:screenOrientation="portrait"
            />

        <activity
            android:name=".RegistroConductorActivity"
            android:label="@string/title_activity_registro_conductor"
            android:screenOrientation="portrait"
            />




        <activity
            android:name=".Slash"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>